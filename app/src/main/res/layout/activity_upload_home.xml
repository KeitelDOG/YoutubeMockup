<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UploadHomeActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView9"
        android:src="@drawable/scn_11_uploads_home" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnVideos"
        android:alpha="0"
        android:layout_marginLeft="80dp"
        android:layout_below="@+id/btnSideMenu"
        android:layout_alignParentEnd="false"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:onClick="onVideos" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnPlaylists"
        android:alpha="0"
        android:layout_alignTop="@+id/btnVideos"
        android:layout_toEndOf="@+id/btnVideos"
        android:layout_marginLeft="10dp"
        android:onClick="onPlaylists" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnChannels"
        android:alpha="0"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/btnPlaylists"
        android:layout_alignStart="@+id/btnSearch"
        android:onClick="onChannels" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnSearch"
        android:alpha="0"
        android:layout_marginRight="45dp"
        android:onClick="onSearch"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="false"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnSideMenu"
        android:alpha="0"
        android:layout_marginLeft="5dp"
        android:onClick="onSideMenu"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
