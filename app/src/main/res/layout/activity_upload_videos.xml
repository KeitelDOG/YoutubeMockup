<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.megalobiz.youtubemockup.UploadVideosActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:src="@drawable/scn_12_uploads_videos" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnPlaylists"
        android:layout_marginTop="60dp"
        android:alpha="0"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="onPlaylists" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnSideMenu"
        android:alpha="0"
        android:onClick="onSideMenu"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnHome"
        android:alpha="0"
        android:layout_alignTop="@+id/btnChannels"
        android:layout_alignParentStart="true"
        android:onClick="onHome" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnChannels"
        android:alpha="0"
        android:layout_alignTop="@+id/btnPlaylists"
        android:layout_toEndOf="@+id/btnPlaylists"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="20dp"
        android:onClick="onChannels" />

    <Button
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnSearch"
        android:alpha="0"
        android:onClick="onSearch"
        android:layout_marginEnd="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
